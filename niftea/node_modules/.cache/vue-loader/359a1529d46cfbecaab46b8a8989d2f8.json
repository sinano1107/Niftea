\" step=\"1\">\n            メールアドレスの入力\n          </v-stepper-step>\n\n          <v-stepper-content step=\"1\">\n            <v-col\n              cols=\"12\"\n              md=\"4\"\n            >\n              <v-text-field\n                v-model=\"email\"\n                :rules=\"emailRules\"\n                label=\"E-mail\"\n                required\n              ></v-text-field>\n            </v-col>\n            <v-btn color=\"primary\" @click=\"e6 = 2\">OK!</v-btn>\n          </v-stepper-content>\n\n          <v-stepper-step :complete=\"e6 > 2\" step=\"2\">パスワードの入力</v-stepper-step>\n\n          <v-stepper-content step=\"2\">\n            <v-col\n              cols=\"12\"\n              md=\"4\"\n            >\n              <v-text-field\n                v-model=\"password\"\n                type=\"password\"\n\n                label=\"password\"\n                required\n              ></v-text-field>\n            </v-col>\n            <div @click=\"signIn\"><v-btn color=\"primary\">signin!!</v-btn></div>\n            <v-btn text @click=\"e6 = 1\">return</v-btn>\n          </v-stepper-content>\n\n\n        </v-stepper>\n      </v-col>\n    </v-row>\n    <v-footer padless fixed>\n      <v-col\n        class=\"text-center\"\n        cols=\"12\"\n      >\n        {{ new Date().getFullYear() }} — <strong>Vuetify</strong>\n      </v-col>\n    </v-footer>\n  </div>\n</template>\n\n<script>\n  import firebase from 'firebase/app'\n  import 'firebase/database'\n  import 'firebase/storage'\n\n  export default {\n    name: 'Signin',\n    data () {\n      return {\n        e6: 1,\n        valid: false,\n        email: '',\n        password: '',\n        emailRules: [\n          v => !!v || 'E-mail is required',\n          v => /.+@.+/.test(v) || 'E-mail must be valid',\n        ]\n      }\n    },\n    methods: {\n      signIn: function() {\n        firebase.auth().signInWithEmailAndPassword(this.email, this.password).then(\n          () => {\n            alert('Success!')\n            this.$router.push('/seatReservation')\n          },\n          err => {\n            alert(err.message)\n          }\n        )\n      }\n    }\n  }\n</script>\n"]}]}